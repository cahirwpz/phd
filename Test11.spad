)abbrev domain TEST11 Test11
Test11(S : OrderedSet) : Exports == Implementation where
  I ==> Integer

  Exports ==> BasicType with
    if S has Integer then
      foo : List(S) -> List(S)
    else
      bar : List(S) -> List(S)

  Implementation ==> List(S) add
    if S has Integer then
      foo x == x
    else
      bar x == x
