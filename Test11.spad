)abbrev domain TEST11 Test11
Test11(S : OrderedSet, R : BasicType) : Exports == Implementation where
  NNI ==> NonNegativeInteger

  Exports ==> BasicType with
    if S has Integer then
      foo : List(S) -> NNI
    else
      foo : List(R) -> NNI

  Implementation ==> List(S) add
    if S has Integer then
      foo x == #x
    else
      foo x == #x
