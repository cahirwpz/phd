    -- fib : NNI -> NNI

  Implementation ==> add
    import F
    import PI
    import NNI
    import I

    -- foo : I -> F

    foo (a : I) ==
      a2 := a * a
      ma := - a
      r :=
        if a >= 0
          then ma * 2
          else a2 + 2
      r + 3

    -- [BUG] Type checker should remember that {foo : I -> I} has been already
