    -- foo a == a + 1

    foo (a : F) == a + 1.0

    baz : (I, I) -> I

    bar (x, y) ==
      -- [BUG] Cannot remove next line, because branch environment merging
      -- doesn't work.
      nx : Integer
      if x > 0 then
        y := -10
        nx := baz(x, y)
      else
        nx := -x 
      nx + y

    baz (x, y) ==
      x * y

